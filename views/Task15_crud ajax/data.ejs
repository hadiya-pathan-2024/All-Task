<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        th{
            border: 1px solid black;
            padding: 10px;
        }
        td{
            border: 1px solid black;
            padding: 10px;
        }
    </style>
</head>
<body>
    <!-- <form action="/search">
        Search: <input type="text" name="name">
        <input type="submit" name="submit" value="search">
    </form>
    <br><br><br> -->
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Designation</th>
            <th>Email</th>
            <th>Contact</th>
            <th>Gender</th>
            <th>Relation Status</th>
            <th>DOB</th>
            <th>Update</th>
            
        </tr>
        <% users.forEach((entry) => { %>
        <tr>
            <td><%= entry.firstname %></td>
            <td><%= entry.lastname %></td>
            <td><%= entry.designation %></td>
            <td><%= entry.email %></td>
            <td><%= entry.phone %></td>
            <td><%= entry.gender %></td>
            <td><%= entry.relation %></td>
            <td><%= entry.dob %></td>
            <td><a href="/update/<%=entry.id%>">Edit</a></td>
        </tr>   
  <% }) %>  
    </table>
    <br><br><br>
    <center><h2><a href="/task15">APPLY</a></h2></center>
</body>
</html>